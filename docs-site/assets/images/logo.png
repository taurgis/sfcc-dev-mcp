<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 120 120" width="120" height="120">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2C3E50"/>
      <stop offset="100%" style="stop-color:#3498DB"/>
    </linearGradient>
    <linearGradient id="accent" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#E74C3C"/>
      <stop offset="100%" style="stop-color:#F39C12"/>
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="120" height="120" fill="url(#bg)" rx="15"/>
  
  <!-- Main logo circle -->
  <circle cx="60" cy="45" r="25" fill="white" opacity="0.95"/>
  
  <!-- SFCC Commerce symbol -->
  <g transform="translate(45, 30)">
    <!-- Cloud -->
    <path d="M5 15 C2 15, 1 13, 3 11 C3 9, 5 8, 7 9 C8 7, 12 7, 13 9 C15 8, 17 10, 15 12 C17 12, 17 15, 15 15 Z" 
          fill="url(#bg)" opacity="0.8"/>
    <!-- Cart -->
    <g transform="translate(3, 11)" fill="url(#bg)" opacity="0.9">
      <rect x="2" y="2" width="6" height="4" rx="0.5" fill="none" stroke="url(#bg)" stroke-width="1"/>
      <circle cx="3.5" cy="8" r="0.8"/>
      <circle cx="6.5" cy="8" r="0.8"/>
      <line x1="1" y1="1" x2="2.5" y2="1" stroke="url(#bg)" stroke-width="1"/>
    </g>
  </g>
  
  <!-- AI Neural Network -->
  <g transform="translate(20, 20)" opacity="0.7">
    <line x1="10" y1="35" x2="70" y2="35" stroke="url(#accent)" stroke-width="2"/>
    <line x1="15" y1="40" x2="65" y2="40" stroke="url(#accent)" stroke-width="1.5"/>
    <line x1="10" y1="45" x2="70" y2="45" stroke="url(#accent)" stroke-width="2"/>
    
    <circle cx="20" cy="35" r="2.5" fill="url(#accent)"/>
    <circle cx="35" cy="40" r="2.5" fill="url(#accent)"/>
    <circle cx="50" cy="45" r="2.5" fill="url(#accent)"/>
    <circle cx="65" cy="35" r="2.5" fill="url(#accent)"/>
  </g>
  
  <!-- Text -->
  <text x="60" y="85" font-family="Arial, sans-serif" font-size="16" font-weight="bold" 
        fill="white" text-anchor="middle">SFCC</text>
  <text x="60" y="100" font-family="Arial, sans-serif" font-size="12" 
        fill="white" text-anchor="middle" opacity="0.9">Dev MCP</text>
  
  <!-- AI badge -->
  <circle cx="95" cy="25" r="12" fill="url(#accent)" opacity="0.9"/>
  <text x="95" y="29" font-family="Arial, sans-serif" font-size="9" font-weight="bold" 
        fill="white" text-anchor="middle">AI</text>
</svg>
